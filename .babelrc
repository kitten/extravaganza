{
  "presets": [
    ["env", {
      "loose": true
    }],
    "react"
  ],
  "plugins": [
    ["babel-plugin-styled-components", {
      ssr: true,
      minify: true
    }],

    "babel-plugin-polished",
    "transform-object-rest-spread",
    "transform-class-properties",
    "transform-runtime",
    "syntax-dynamic-import"
  ]
}
