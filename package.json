{
  "name": "extravaganza",
  "version": "1.0.0-16",
  "description": "Framework for React-powered presentations",
  "main": "index.js",
  "bin": {
    "extravaganza": "./bin/extravaganza.js"
  },
  "scripts": {
    "prebuild": "rm -rf lib",
    "build": "babel src --out-dir lib",
    "format": "prettier --no-semi --single-quote --write src/**/*.js",
    "precommit": "lint-staged"
  },
  "files": [
    "bin",
    "lib",
    "src",
    "assets",
    "package.json",
    ".editorconfig",
    "yarn.lock",
    "index.js",
    "babel.js",
    "theme.js",
    "themes.js"
  ],
  "lint-staged": {
    "linters": {
      "src/**/*.js": [
        "prettier --no-semi --single-quote --write",
        "git add"
      ]
    }
  },
  "repository": "https://github.com/philpl/extravaganza",
  "author": "Phil Pl√ºckthun <phil@plckthn.me> (https://github.com/philpl)",
  "license": "MIT",
